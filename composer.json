{
    "name": "chinleung/laravel-faker-providers",
    "description": "Custom collection of Laravel Faker providers.",
    "keywords": [
        "chinleung",
        "laravel-faker-providers"
    ],
    "homepage": "https://github.com/chinleung/laravel-faker-providers",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Chin Leung",
            "email": "hello@chinleung.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "~5.8.0|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "ChinLeung\\LaravelFakerProviders\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChinLeung\\LaravelFakerProviders\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
